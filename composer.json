{
  "name": "bzzhh/pezos",
  "description": "Tezos library",
  "type": "library",
  "keywords": [
    "tezos"
  ],
  "autoload": {
    "psr-4": {
      "Bzzhh\\Pezos\\": "src/",
      "Bzzhh\\Pezos\\Generated\\Shell\\": "generated/shell",
      "Bzzhh\\Pezos\\Generated\\Proto\\": "generated/proto",
      "Bzzhh\\Pezos\\Generated\\Mempool\\": "generated/mempool"
    },
    "files": [
      "src/_bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bzzhh\\Pezos\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1|^8.0",
    "ext-sodium": "*",
    "jane-php/open-api-runtime": "^6.3",
    "bzzhh/tzkt-php-client": "dev-main",
    "tuupola/base58": "^2.1",
    "simplito/elliptic-php": "^1.0",
    "symfony/config": "^5.3",
    "symfony/dependency-injection": "^5.3",
    "symfony/http-kernel": "^5.3",
    "symfony/property-access": "^5.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^4.6",
    "jane-php/open-api-3": "^6.3",
    "symfony/var-dumper": "^5.2",
    "php-http/client-common": "^2.3",
    "php-http/message": "^1.11",
    "php-http/discovery": "^1.13",
    "nyholm/psr7": "^1.4",
    "php-http/curl-client": "^2.2"
  },
  "archive": {
    "exclude": [
      "*",
      ".*",
      "*/",
      "!/src/*",
      "!/generated/*",
      "!/composer.json",
      "!/README.md"
    ]
  }
}
