{
    "name": "sortpif/pezos",
    "description": "Tezos library",
    "type": "library",
    "keywords": [
        "tezos"
    ],
    "autoload": {
        "psr-4": {
            "Pezos\\": "src/"
        },
        "files": [
            "src/_bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "require": {
        "php": "^7.4",
        "bitwasp/buffertools": "^0.5.7",
        "bitwasp/bitcoin": "^1.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "version": "1.0.0-alpha",
    "archive": {
        "exclude": [
            "*",
            ".*",
            "*/",
            "!/src/*",
            "!/composer.*",
            "!/README.md"
        ]
    }
}
