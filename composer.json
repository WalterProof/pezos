{
  "name": "bzzhh/pezos",
  "description": "Tezos library",
  "type": "library",
  "keywords": [
    "tezos"
  ],
  "autoload": {
    "psr-4": {
      "Bzzhh\\Pezos\\": "src/",
      "Bzzhh\\Pezos\\Generated\\Shell\\": "generated/shell",
      "Bzzhh\\Pezos\\Generated\\Proto\\": "generated/proto",
      "Bzzhh\\Pezos\\Generated\\Mempool\\": "generated/mempool"
    },
    "files": [
      "src/_bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bzzhh\\Pezos\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4",
    "ext-sodium": "*",
    "bitwasp/buffertools": "^0.5.7",
    "bitwasp/bitcoin": "^1.0",
    "jane-php/open-api-runtime": "^6.3",
    "symfony/framework-bundle": "^5.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^4.6",
    "jane-php/open-api-3": "^6.3",
    "symfony/var-dumper": "^5.2",
    "symfony/dotenv": "^5.2",
    "php-http/client-common": "^2.3",
    "php-http/message": "^1.11",
    "php-http/discovery": "^1.13",
    "nyholm/psr7": "^1.4",
    "php-http/curl-client": "^2.2"
  },
  "archive": {
    "exclude": [
      "*",
      ".*",
      "*/",
      "!/src/*",
      "!/generated/*",
      "!/composer.json",
      "!/README.md"
    ]
  }
}
