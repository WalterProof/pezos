{
  "name": "sjnnya/pezos",
  "description": "Tezos library",
  "type": "library",
  "keywords": [
    "tezos"
  ],
  "autoload": {
    "psr-4": {
      "Pezos\\": "src/"
    },
    "files": [
      "src/_bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4",
    "bitwasp/buffertools": "^0.5.7",
    "bitwasp/bitcoin": "^1.0",
    "symfony/http-client": "^5.2",
    "symfony/dotenv": "^5.2",
    "ext-sodium": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^4.6"
  },
  "version": "1.0.0-alpha",
  "archive": {
    "exclude": [
      "*",
      ".*",
      "*/",
      "!/src/*",
      "!/composer.*",
      "!/README.md"
    ]
  }
}
